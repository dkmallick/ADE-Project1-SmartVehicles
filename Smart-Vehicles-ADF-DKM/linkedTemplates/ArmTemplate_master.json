{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "Smart-Vehicles-ADF-DKM"
		},
		"SmartVehicles_ADLS_LS_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'SmartVehicles_ADLS_LS'"
		},
		"S3SmartVehiclesDataLS_properties_typeProperties_accessKeyId": {
			"type": "object",
			"defaultValue": {
				"type": "AzureKeyVaultSecret",
				"store": {
					"referenceName": "SmartVehicles_KeyVault_LS",
					"type": "LinkedServiceReference"
				},
				"secretName": "s3accesskey"
			}
		},
		"SmartVehicles_ADLS_LS_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://project1smartvehiclesdkm.dfs.core.windows.net/"
		},
		"SmartVehicles_KeyVault_LS_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://smartvehicles-keyvault.vault.azure.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "Smart-Vehicles-ADF-DKM_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"SmartVehicles_ADLS_LS_accountKey": {
						"value": "[parameters('SmartVehicles_ADLS_LS_accountKey')]"
					},
					"SmartVehicles_ADLS_LS_properties_typeProperties_url": {
						"value": "[parameters('SmartVehicles_ADLS_LS_properties_typeProperties_url')]"
					},
					"SmartVehicles_KeyVault_LS_properties_typeProperties_baseUrl": {
						"value": "[parameters('SmartVehicles_KeyVault_LS_properties_typeProperties_baseUrl')]"
					},
					"S3SmartVehiclesDataLS_properties_typeProperties_accessKeyId": {
						"value": "[parameters('S3SmartVehiclesDataLS_properties_typeProperties_accessKeyId')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}